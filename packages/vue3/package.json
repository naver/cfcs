{
  "name": "@cfcs/vue3",
  "version": "0.0.23",
  "description": "CFCs(Cross Framework Components) are modules that convert components into framework usage for Vue 3",
  "main": "dist/cfcs.cjs.js",
  "module": "dist/cfcs.esm.js",
  "types": "declaration/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "rollup -c && npm run declaration && print-sizes ./dist ",
    "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/naver/cfcs"
  },
  "author": {
    "name": "NAVER Corp."
  },
  "keywords": [
    "cfcs",
    "cfc",
    "vue3",
    "vue",
    "vue-hook",
    "vue-hooks",
    "vue-use",
    "scroll",
    "reactive",
    "hooks",
    "use"
  ],
  "files": [
    "./*",
    "dist/*"
  ],
  "dependencies": {
    "@cfcs/core": "~0.0.23"
  },
  "devDependencies": {
    "@egjs/build-helper": "^0.1.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "print-sizes": "^0.1.0",
    "typescript": "^4.7.4",
    "vue": "^3.2.21"
  }
}
