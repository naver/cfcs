{
  "name": "@cfcs/svelte",
  "version": "0.0.24",
  "description": "CFCs(Cross Framework Components) are modules that convert components into framework usage for Svelte",
  "main": "dist/cfcs.cjs.js",
  "module": "dist/cfcs.esm.js",
  "svelte": "dist/cfcs.esm.js",
  "types": "declaration/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "build": "rollup -c rollup.build.config.js && npm run declaration",
    "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/naver/cfcs"
  },
  "author": {
    "name": "NAVER Corp."
  },
  "keywords": [
    "cfcs",
    "cfc",
    "svelte",
    "svelte-hook",
    "svelte-hooks",
    "svelte-use",
    "scroll",
    "reactive",
    "hooks",
    "use"
  ],
  "files": [
    "./*",
    "dist/*"
  ],
  "dependencies": {
    "@cfcs/core": "~0.0.24"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@egjs/build-helper": "^0.1.2",
    "@pyoner/svelte-ts-preprocess": "^1.2.1",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/svelte": "^3.0.3",
    "@types/jest": "^26.0.19",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "print-coveralls": "^1.2.2",
    "rollup": "^1.12.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^5.1.2",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.31.0",
    "svelte-jester": "^1.3.0",
    "svelte-preprocess": "^4.6.1",
    "ts-jest": "^26.4.4",
    "tslib": "^1.10.0",
    "typescript": "^4.1.6"
  }
}
