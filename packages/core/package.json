{
  "name": "@cfcs/core",
  "version": "0.0.24",
  "description": "CFCs(Cross Framework Components) are modules that convert components into framework usage",
  "main": "dist/cfcs.cjs.js",
  "module": "dist/cfcs.esm.js",
  "types": "declaration/index.d.ts",
  "scripts": {
    "start": "rollup -c -w",
    "build": "rm -rf ./declaration && rollup -c && npm run declaration",
    "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json",
    "test": "jest --watchAll",
    "lint": "eslint ./src/ --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naver/cfcs"
  },
  "author": "NAVER Crop.",
  "license": "MIT",
  "keywords": [
    "cfc",
    "cfcs",
    "react",
    "angular",
    "svelte",
    "vue",
    "vue2",
    "vue3"
  ],
  "files": [
    "./*",
    "dist/*"
  ],
  "dependencies": {
    "@egjs/component": "^3.0.4"
  },
  "devDependencies": {
    "@egjs/build-helper": "^0.1.2",
    "@types/sinon": "^10.0.13",
    "jest": "^29.3.1",
    "sinon": "^15.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.7.4"
  }
}
